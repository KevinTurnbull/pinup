<fieldset>
  <%= f.label :name, "Name" %><br />
  <%= f.text_field :name, class: 'form-control autocomplete-artist ui-widget'%>
  <%= f.check_box :_destroy %>
  <%= f.label :_destroy, "Remove Artist" %>
</fieldset>

<script>
  $(function() {
    var availableTags = <%= raw all_by_name_array %>;
    $( ".autocomplete-artist" ).autocomplete({
      source: availableTags
    });
  });
  </script>