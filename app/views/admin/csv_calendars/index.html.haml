%h1 Listing csv_calendars
%table
  %thead
    %tr
      %th{:colspan => "3"}
  %tbody
    - csv_calendars.each do |csv_calendar|
      %tr
        %td= link_to 'Show', admin_csv_calendar_path(csv_calendar)
        %td= link_to 'Edit', edit_admin_csv_calendar_path(csv_calendar)
        %td= link_to 'Destroy', admin_csv_calendar_path(csv_calendar), method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Csv calendar', new_csv_calendar_path
